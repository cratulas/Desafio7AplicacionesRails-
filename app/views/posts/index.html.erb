<div class="container">
<h1 class="text-center">Blog</h1>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col">Date_of_admission</th>
            <th scope="col">Content</th>
            <th scope="col">Image_url</th>
        </tr>
    </thead>
    <tbody>
        <% @posts.each do |p|%>
            <tr scope="row">
                <td><%= p.id %></td>
                <td><%= p.title %></td>
                <td><%= p.created_at %></td>
                <td><%= p.content %></td>
                <td><%= p.image_url %></td>
        <% end %>
    </tbody>
</table>

</div>